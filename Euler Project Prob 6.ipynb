   ],
   "source": [
    "#합의 제곱\n",
    "sums = sum(range(1,101))\n",
    "\n",
    "square_of_sums = (sums*sums)\n",
    "\n",
    "square_of_sums"
   ]
  },
  {
   "cell_type": "code",
